{
    "name": "optilab/wp-toplist",
    "description": "WordPress toplist module to be integrated with theme.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Hitankar Ray",
            "email": "hitankar@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hitankar/WPMetaBoxBuilder.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hitankar/wp-db.git"
        }
    ],
    "require": {
        "optilab/wp-meta-box-builder": "dev-master",
        "optilab/wp-db": "dev-master"
    },
    "autoload":     {
        "psr-4": {
            "OptiLab\\TopList\\": "app/"
        },
        "files": [
            "app/postfilter.php",
            "setup.php",
            "app/Taxonomies/toplist-item-type-taxonomy.php",
            "app/PostTypes/toplist-item-post-type.php",
            "app/PostTypes/toplist-item-review-post-type.php",
        ]
    },
}
